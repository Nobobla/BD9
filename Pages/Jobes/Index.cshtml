@page
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model BD9.Pages.Jobes.IndexModel

<h2>Список Должностей</h2>
<table>
    <tr><th>Наименование Должности</th></tr>
    @foreach (var user in Model.Jobs)
    {
            <tr>
                <td>@user.JobName</td>
            </tr>
    }
</table>
