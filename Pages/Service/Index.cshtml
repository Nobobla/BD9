@page
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model BD9.Pages.Service.IndexModel

<h2>Список Услуг</h2>
<table>
    <tr><th>Наименование услуги</th><th>Номер модели</th><th>Цена</th></tr>
    @foreach (var user in Model.Servises)
    {
        <tr>
            <td>@user.ServiseName</td>
            <td>@user.ModelId</td>
            <td>@user.Price</td>
        </tr>
    }
</table>